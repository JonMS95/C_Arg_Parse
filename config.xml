<?xml version="1.0" encoding="UTF-8"?>
<config>
    <Project_data
        version_major="1"
        version_minor="2"
        version_mode="RELEASE"
        URL="https://github.com/JonMS95/C_Arg_Parse"
        type="library"
        language="C"
        library_name="GetOptions"
    />
    <!-- Directory structure -->
    <Directories>
        <Dynamic_libraries/>
        <API/>
        <Dependency_files>
            <Header_files/>
            <Dynamic_libraries/>
        </Dependency_files>
        <Tests>
            <Dependency_files>
                <Dynamic_libraries/>
                <Header_files/>
            </Dependency_files>
            <Executable_files/>
        </Tests>
    </Directories>
    
    <!-- Common shell files location -->
    <Common_shell_files
        version_major="1"
        version_minor="1"
        version_mode="RELEASE"
        local_path="~/Desktop/scripts/C/C_Common_shell_files"
        URL="https://github.com/JonMS95/C_Common_shell_files"
    />
    
    <!-- Dependencies -->
    <!-- Paths to dependencies from other projects. -->
    <Dependencies Dest="Dependency_files">
        <C_Severity_Log
            version_major="1"
            version_minor="1"
            version_mode="RELEASE"
            local_path="~/Desktop/scripts/C/C_Severity_Log"
            URL="https://github.com/JonMS95/C_Severity_Log"
        />
    </Dependencies>
    
    <!-- Tests -->
    <!-- Paths to dependency files for the testing executable. -->
    <Tests>
        <Dependencies Dest="Tests/Dependency_files">
            <C_Severity_Log
                version_major="1"
                version_minor="1"
                version_mode="RELEASE"
                local_path="~/Desktop/scripts/C/C_Severity_Log"
                URL="https://github.com/JonMS95/C_Severity_Log"
            />
            <C_Arg_Parse
                version_major="1"
                version_minor="2"
                version_mode="RELEASE"
                local_path="~/Desktop/scripts/C/C_Arg_Parse"
                URL="https://github.com/JonMS95/C_Arg_Parse"
            />
        </Dependencies>
    </Tests>
</config>
